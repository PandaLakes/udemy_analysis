                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    C.course_id,
    S.subject_id,
    L.level_id,
    CASE WHEN is_paid = 'True' THEN 1 ELSE 0 END AS is_paid,
    CAST(price AS FLOAT) AS price,
    CAST(num_subscribers AS INT) AS num_subscribers,
    CAST(num_reviews AS INT) AS num_reviews,
    CAST(num_lectures AS INT) AS num_lectures,
    CAST(content_duration AS FLOAT) AS content_duration,
    CAST(published_timestamp AS DATE) AS published_date,
    CAST(price * num_subscribers AS FLOAT) AS revenue_per_course,
    CAST((num_reviews * 1.0 / NULLIF(num_subscribers, 0)) AS FLOAT) AS engagement_ratio
FROM [STAGING].[COURSE] AS SC
JOIN DW.Subject_Dim AS S ON SC.subject = S.subject_name
JOIN DW.Level_Dim AS L ON SC.level = L.level_name
JOIN DW.Course_Dim AS C ON SC.course_id = C.course_id
WHERE SC.subject IS NOT NULL 
  AND SC.level IS NOT NULL 
  AND SC.course_id IS NOT NULL
  AND num_subscribers IS NOT NULL 
  AND num_reviews IS NOT NULL;</property>
